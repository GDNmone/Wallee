<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Wallee"
                    xmlns:views="clr-namespace:Wallee.Views"
                    xmlns:viewModels="clr-namespace:Wallee.ViewModels">
    <DataTemplate DataType="{x:Type viewModels:ViewModelMorePhoto}">
        <views:ViewMorePhoto DataContext="{Binding}" />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:ViewModelMain}">
        <views:ViewMain DataContext="{Binding}" />
    </DataTemplate>
    <!--<DataTemplate DataType="{x:Type viewModels:ViewModelMain}">
        <views:ViewMain DataContext="{Binding}" />
    </DataTemplate>-->

</ResourceDictionary>