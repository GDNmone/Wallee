<Window x:Class="Wallee.CustomControls.WindowMain"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewModels="clr-namespace:Wallee.ViewModels"
        xmlns:views="clr-namespace:Wallee.Views"
        mc:Ignorable="d"
        WindowStyle="None"
        Style="{StaticResource MyWindowStyle}"
        Height="450" Width="800">
    <Window.DataContext>
        <viewModels:ViewModelMain />
    </Window.DataContext>
    <Window.Resources>
        <DataTemplate DataType="{x:Type viewModels:ViewModelMain}">
            <views:ViewImages></views:ViewImages>
        </DataTemplate>
    </Window.Resources>
    <ContentPresenter />

</Window>