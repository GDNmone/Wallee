<Window x:Class="Wallee.Windows.WindowMain"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wallee"
        xmlns:windows="clr-namespace:Wallee.Windows"
        mc:Ignorable="d"
        WindowStyle="None"
        Style="{StaticResource MyWindowStyle}"
        Height="450" Width="800">
    <ContentPresenter Content="{Binding RelativeSource={RelativeSource Mode=FindAncestor,AncestorType=Window}, Path=SourcePage}" />

</Window>