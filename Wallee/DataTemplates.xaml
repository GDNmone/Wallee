<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Wallee"
                    xmlns:viewModels="clr-namespace:Wallee.ViewModels"
                    xmlns:views="clr-namespace:Wallee.Views">
    <DataTemplate DataType="{x:Type viewModels:ViewModelCategories}">
        <views:ViewCategories DataContext="{Binding}" />
    </DataTemplate> 
    <DataTemplate DataType="{x:Type viewModels:ViewModelMorePhoto}">
        <views:ViewMorePhoto DataContext="{Binding}" />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:ViewModelLostConnection}">
        <views:ViewLoctConnection DataContext="{Binding}" />
    </DataTemplate>
</ResourceDictionary>